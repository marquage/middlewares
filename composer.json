{
    "name": "marquage/middlewares",
    "type": "library",
    "description": "middlewares for Marques",
    "keywords": [
        "marquage",
        "middlewares"
    ],
    "homepage": "https://github.com/marquage/middlewares",
    "license": "MIT",
    "authors": [
        {
            "name": "Farhang E",
            "email": "farhang@riseup.net",
            "homepage": "https://github.com/marquage",
            "role": "Developer(ish)"
        }
    ],
    "require": {
        "php": "~7.3",
        "cocur/slugify": "^3.2",
        "psr/container": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "rdlowrey/auryn": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit" : "~8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Marquage\\Middlewares\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marquage\\Middlewares\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
